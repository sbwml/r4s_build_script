### Init
CONFIG_TARGET_rockchip=y
CONFIG_TARGET_rockchip_armv8=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r5c=y
CONFIG_TARGET_DEVICE_rockchip_armv8_DEVICE_friendlyarm_nanopi-r5s=y

### Basic
CONFIG_ALL_KMODS=y
CONFIG_ALL_NONSHARED=y
CONFIG_KERNEL_BUILD_DOMAIN="cooluc.com"
CONFIG_KERNEL_BUILD_USER="admin"
CONFIG_PACKAGE_autocore-arm=y
CONFIG_PACKAGE_bind-host=y
CONFIG_PACKAGE_block-mount=y
CONFIG_PACKAGE_coremark=y
CONFIG_PACKAGE_default-settings=y
CONFIG_TARGET_KERNEL_PARTSIZE=16
CONFIG_TARGET_ROOTFS_PARTSIZE=944
CONFIG_COREMARK_NUMBER_OF_THREADS=4
# CONFIG_KERNEL_KALLSYMS is not set

### Video Support
